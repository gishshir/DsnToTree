<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" >


	<xs:element name="settings" type="Settings" />

	<xs:complexType name="Settings">
		<xs:sequence>
			<xs:element name="bdd" type="Bdd" minOccurs="0" maxOccurs="1" />
			<xs:element name="norme" type="Norme" minOccurs="0" maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Norme">
	<xs:sequence>
			<xs:element name="dsnnormefile" type="xs:string" />
	</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Bdd">
		<xs:sequence>
			<xs:element name="bddAccesses" type="BddAccesses" />
			<xs:element name="sqls" type="Sqls" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="BddAccesses">
		<xs:sequence>
			<xs:element name="oracleBdd" type="OracleBddAccess" maxOccurs="unbounded" minOccurs="1" />
			<xs:element name="postgreBdd" type="PostGreBddAccess" maxOccurs="unbounded" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="OracleBddAccess">
		<xs:sequence>
			<xs:element name="host" type="xs:string" />
			<xs:element name="port" type="xs:int" />
			<xs:element name="instance" type="xs:string" />
			<xs:element name="user" type="xs:string" />
			<xs:element name="password" type="xs:string" />
			<xs:element name="defaut" type="xs:boolean" maxOccurs="1" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PostGreBddAccess">
		<xs:sequence>
			<xs:element name="host" type="xs:string" />
			<xs:element name="port" type="xs:int" />
			<xs:element name="instance" type="xs:string" />
			<xs:element name="user" type="xs:string" />
			<xs:element name="password" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Sqls">
		<xs:sequence>
			<xs:element name="sql" type="Sql" maxOccurs="unbounded" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Sql">
		<xs:sequence>
			<xs:element name="name" type="xs:string" />
			<xs:element name="value" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	
</xs:schema>
